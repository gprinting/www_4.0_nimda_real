<!doctype html>
<html lang="ko">
<head>
    <title>[TPH_Vmain_title]</title>
    <meta charset="utf-8">
    [TPH_I/common/common.html]
    <script src="[TPH_Vdesign_dir]/js/uploader/plupload.full.min.js"></script>
</head>
<body>

<div id="container">
    	
   	   <!-- TOP NAVIGATION --> 
       [TPH_I/common/header.html]

	   <!-- LEFT NAVIGATION --> 
       [TPH_Vleft]

       <!-- CONTENT --> 
        <section id="page-content">            

        <!-- 히스토리 -->
        [TPH_I/common/history.html]
            
            <!-- 본문 컨텐츠  시작-->
            
            <!-- 탭 박스 1 기본검색 -->
            <div class="tb_group clear">
    			<ul class="tab_box mt25">
    			</ul>
			    <form>
			        <div class="tab_box_con">
					    <div class="form-group">
			                <label class="control-label fix_width79 tar">판매채널</label><label class="fix_width20 fs14 tac">:</label>
        			        <select id="sell_site" class="fix_width180" onchange="loadDeparInfo();">
        		              [TPH_Vsell_site]
                            </select>
					    </div>
					    
					    <div class="form-group">
			                <label class="control-label fix_width79 tar">팀구분</label><label class="fix_width20 fs14 tac">:</label>
                            <select id="depar_code" class="fix_width180">
        		            </select>
					    </div>
					
    					<div class="form-group">
        			        <label class="control-label fix_width79 tar">회원명</label><label class="fix_width20 fs14 tac">:</label>        			        
                            <input type="text" id="office_nick" onkeydown="searchOfficeNick(event, this.value, '');" class="input_co2 fix_width173" value="" placeholder="검색창 팝업 사용"> 
                            <input type="hidden" id="member_seqno">
    					</div>
					
    					<div class="form-group">
    						<label class="control-label fix_width79 tar">일자별</label><label class="fix_width20 fs14 tac">:</label>
                            [TPH_Vdatepicker_html]
    					</div>
    					
    					<div class="form-group">
						    <label class="control-label fix_width79 tar">견적상태</label><label class="fix_width20 fs14 tac">:</label>
                            <select class="fix_width150" disabled>
                              <option value="">견적</option>
                            </select>
                            <select id="status" class="fix_width150">
                              <option value="">상태(전체)</option>
                              <option value="대기">대기</option>
                              <option value="중">중</option>
                              <option value="완료">완료</option>
                              <option value="수정">수정</option>
                            </select>
					    </div>
			        </div>
			    </form>
            </div> <!-- 탭 박스 1 기본검색 -->
           
             <!-- 상세검색버튼 --> 
            <div>
                <p class="btn-lg red_btn">
                    <a style="cursor: pointer;" onclick="searchEsti();">선택 조건으로 검색</a>					
                </p>
            <div>
            
            <!-- 탭 박스 3 TAB -->
            <div class="tb_group mt25">
                <div class="tabbable">                  	
                    <div class="tab-content">                     	
                 	    <div class="tab-pane active" id="tab_list1">
                            <div class="table-body">
                                <div class="table_basic">
				                    <ul class="table_top">
				                    	<li class="sel">
				                    	</li>
				                    	<li class="sel tar">
                                          <span class="fs13" style="color:red;">*분홍색 줄은 예상주문일이 다가온 견적입니다.</span>
				                    	</li>
				                    </ul>
				                    <table class="table fix_width100f">
                                        <thead>
							                <tr>
				                      			<th class="bm2px"><input type="checkbox" class="check_box" id="allCheck" onclick="allCheck();"></t:>
							                    <th class="bm2px">견적요청일</th>
							                    <th class="bm2px">회원명</th>
							                    <th class="bm2px">견적번호</th>
							                    <th class="bm2px">문의제목</th>
							                    <th class="bm2px">견적금액</th>
							                    <th class="bm2px">상태</th>
							                    <th class="bm2px">관리</th>
							                </tr>
                                        </thead>
                                        <tbody id="list">
                                        </tbody>
				                   	</table>
				                   	<p class="p_num_b fs12">
				                   	    <button class="btn btn_pu fix_width120 fix_height30 bred fs12" onclick="quiescenceProc();">선택견적삭제</button>
                                        <label class="fix_width20">&nbsp;</label>
				                   	    Showing 1 to 5 of 30 entries
				                        <select name="list_set" class="fix_width55" onchange="showPageSetting(this.value);">
				                   	        <option>5</option>
				                   	        <option>10</option>
				                   	        <option>20</option>
				                            <option selected="selected">30</option>
				                   	    </select>
				                   	</p>
				                   	<div class="tac clear" id="page">
							        </div>					                   		
                                </div>
                            </div>
                        </div>
                    </div>                     	 
                </div>                    
            </div> <!-- 탭 박스 3 TAB -->
<form name="frm" id="frm">
    <input type="hidden" id="seqno" name="seqno">
</form>
            <!-- 견적 View -->
            <div class="tb_group clear" id="esti_cont">
            </div> <!-- 견적 View -->
        </section>
</div>
</body>
</html>
